public class ExemploForEncadeado {  
   public static void main(String[] args) {  
      
      //loop de i  
      for(int i = 1; i <= 3; i++) {  
         
         //loop de j  
         for(int j = 1; j <=3; j++) {  
      
              System.out.println(i + " " + j);  
      
         } // fim de j  
     } // fim de i  
  }  
}  
